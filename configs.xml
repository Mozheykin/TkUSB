<?xml version="1.0"?>
<TkUSB>
     <title>App load on xml</title>
     <geometry>350x350</geometry>
     <main_color>gray</main_color>
     <buttons>
          <PIN_1>
               <bg>gray</bg>
               <text>PIN 1</text>
               <font>Courier 9</font>
               <width>10</width>
               <bd>4</bd>
               <relief>raised</relief>
               <command>all_on</command>
               <relx>0.15</relx>
               <rely>0.25</rely>
               <anchor>CENTER</anchor>
          </PIN_1>
          <PIN_2>
               <bg>gray</bg>
               <text>PIN 2</text>
               <font>Courier 9</font>
               <width>10</width>
               <bd>4</bd>
               <relief>raised</relief>
               <command>all_off</command>
               <relx>0.15</relx>
               <rely>0.35</rely>
               <anchor>CENTER</anchor>
          </PIN_2>
          <PIN_3>
               <bg>gray</bg>
               <text>PIN 3</text>
               <font>Courier 9</font>
               <width>10</width>
               <bd>4</bd>
               <relief>raised</relief>
               <command>change_collor</command>
               <relx>0.15</relx>
               <rely>0.45</rely>
               <anchor>CENTER</anchor>
          </PIN_3>
     </buttons>
     <labels>
          <LABEL1>
               <text>Hello i'm label 1</text>
               <relx>0.4</relx>
               <rely>0.15</rely>
               <anchor>CENTER</anchor>
          </LABEL1>
     </labels>
     <entrys>
          <ENTRY1>
               <width>10</width>
               <relx>0.65</relx>
               <rely>0.65</rely>
          </ENTRY1>
     </entrys>
     <checkbuttons>
          <CHECKBUTTON1>
               <text>Checkbutton1</text>
               <width>15</width>
               <relx>0.65</relx>
               <rely>0.75</rely>
          </CHECKBUTTON1>
     </checkbuttons>
     <img_buttons>
          <CHB1>
               <type>check</type>
               <image>yes</image>
               <bd>0</bd>
               <command>change_checkbutton_position</command>
               <highlightthickness>0</highlightthickness>
               <activebackground>gray</activebackground>
               <bg>gray</bg>
               <subsample>6</subsample>
               <relx>0.25</relx>
               <rely>0.95</rely>
               <anchor>CENTER</anchor>
          </CHB1>
          <CHB2>
               <type>tick</type>
               <image>yes</image>
               <bd>0</bd>
               <command>change_checkbutton_position</command>
               <highlightthickness>0</highlightthickness>
               <activebackground>gray</activebackground>
               <bg>gray</bg>
               <subsample>6</subsample>
               <relx>0.75</relx>
               <rely>0.50</rely>
               <anchor>CENTER</anchor>
          </CHB2>
     </img_buttons>
     <comboboxs>
          <CB1>
               <width>10</width>
               <relx>0.35</relx>
               <rely>0.55</rely>
               <anchor>CENTER</anchor>
          </CB1>
     </comboboxs>

</TkUSB>