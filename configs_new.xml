<?xml version="1.0"?>
<TkUSB>
     <title>App load on xml</title>
     <geometry>570x300</geometry>
     <main_color>gray</main_color>
     <notebooks>
          <NB1>
               <interaction>None</interaction>
               <on_what>root</on_what>
               <expand>True</expand>
               <fill>BOTH</fill>
               <frames>USB Switch 8,USB Switch 4</frames>
               <names_frames>NB11,NB12</names_frames>
               <width>200</width>
               <height>250</height>
               <padding>0</padding>
               <relx>0</relx>
               <rely>0</rely>
          </NB1>
          <NB2>
               <interaction>None</interaction>
               <on_what>root</on_what>
