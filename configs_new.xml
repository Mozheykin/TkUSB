               <interaction>CB1</interaction>
               <on_what>NB11</on_what>
               <bg>gray</bg>
               <font>Courier 9</font>
               <width>10</width>
               <bd>4</bd>
               <relief></relief>
               <saved>PIN_1</saved>
               <text_swich>STATUS OFF,STATUS ON</text_swich>
               <relx>0.15</relx>
               <rely>0.10</rely>
               <command>change_collor</command>
               <collors>red,green</collors>
